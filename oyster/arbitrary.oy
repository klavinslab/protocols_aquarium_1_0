argument
  x: generic
end

place p1
  protocol: "oyster/arbitrary.pl"
  group: aquarium_user
  marked: true
  argument
    x: x
    y: keys(x)
  end
  start: now()
  window: hours(1)
end

place p2
  protocol: "oyster/arbitrary.pl"
  group: aquarium_user
  marked: true
  argument
    x: x
 end
  start: now()
  window: hours(1)
end

wire (p1,"output") => (p2,"y")

transition [ p1 ] => [ p2 ] when !error(0) && completed(0) end
transition [ p2 ] => [] when !error(0) && completed(0) end