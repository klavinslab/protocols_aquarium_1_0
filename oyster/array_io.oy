place p1
  protocol: "oyster/io.pl"
  group: aquarium_user
  marked: true
  argument
    u: [ 0, 0, 0 ]
  end
  start: now()
  window: hours(1)
end

place p2
  protocol: "oyster/io.pl"
  group: aquarium_user
  start: now()
  window: hours(1)
end

wire (p1,"plate_ids") => (p2,"plate_ids")

transition [ p1 ] => [ p2 ] when !error(0) && completed(0) end


